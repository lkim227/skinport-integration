{
    "compilerOptions": {
      "target": "ES2020",
      "module": "Node16",             
      "moduleResolution": "Node16",    // Set moduleResolution to Node16
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "outDir": "./dist"
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  